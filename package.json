{
  "name": "react-native-starter",
  "version": "0.0.4",
  "description": "React Native Starter",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "reinstall": "rm -rf node_modules && yarn install",
    "pod": "cd ios && USE_FABRIC=1 RCT_NEW_ARCH_ENABLED=1 USE_FLIPPER=1 pod install && cd ..",
    "pod-release": "cd ios && USE_FABRIC=1 RCT_NEW_ARCH_ENABLED=1 USE_FLIPPER=0 pod install && cd .."
  },
  "dependencies": {
    "@notifee/react-native": "^4.0.1",
    "react": "18.0.0",
    "react-native": "0.69.0-rc.6",
    "react-query": "^3.34.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jiggag-rnlint": "^0.0.8",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-module-resolver": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-react-native": "^4.0.0",
    "flipper-plugin-react-query-native-devtools": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "prettier": "^2.7.1",
    "react-native-flipper": "^0.129.0",
    "react-query-native-devtools": "^3.0.2",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
