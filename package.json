{
  "name": "react-native-starter",
  "version": "0.0.1",
  "description": "React Native Starter",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest .",
    "lint": "eslint . --ext .js,.ts,.tsx;stylelint 'src/**/*.js' 'src/**/*.ts' 'src/**/*.tsx'",
    "reinstall": "rm -rf node_modules && yarn install",
    "pod": "cd ios && pod install && cd ..",
    "postinstall": "yarn pod",
    "android:dev-build": "rnsemup && cd android && fastlane build && cd .. && yarn process-kill-java && yarn open-android-build-folder",
    "android:live-build": "rnsemup && cd android && fastlane build --env production && yarn process-kill-java && cd .. && yarn open-android-build-folder",
    "android:live-bundle": "rnsemup && cd android && fastlane bundle --env production && yarn process-kill-java && cd .. && yarn open-android-build-folder",
    "open-android-build-folder": "open ./android/app/build/outputs/apk/",
    "process-kill-java": "pkill -9 -f java"
  },
  "dependencies": {
    "@bugsnag/react-native": "^7.5.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/datetimepicker": "^3.0.4",
    "@react-native-community/masked-view": "^0.1.8",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-navigation/native": "^5.8.0",
    "@react-navigation/stack": "^5.10.0",
    "apisauce": "^2.0.0",
    "immer": "^7.0.14",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-config": "^1.3.3",
    "react-native-flipper": "^0.63.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-semver-update": "^1.1.2",
    "react-native-ui-lib": "^5.15.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-flipper": "^1.4.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.43",
    "@types/react-native": "^0.63.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "stylelint": "^13.3.1",
    "stylelint-config-react-native-styled-components": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-react-native": "^2.2.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./node_modules/react-native-ui-lib/jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@?react-navigation)"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "stylelint '**/*.js' '**/*.ts' '**/*.tsx'",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
